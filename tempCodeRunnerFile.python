def hauteur(arbre):
    if not arbre:  # Cas où l'arbre est une feuille
        return 0
    else:
        return 1 + max(hauteur(sous_arbre) for sous_arbre in arbre)

def taille(arbre):
    if not arbre:  # Cas où l'arbre est une feuille
        return 1
    else:
        return 1 + sum(taille(sous_arbre) for sous_arbre in arbre)


# tests

assert hauteur([]) == 0
assert hauteur([[], [], [[]]]) == 2

assert taille([]) == 1
assert taille([[], [], [[]]]) == 5
